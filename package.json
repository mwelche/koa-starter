  "author": "Mathieu Welche",
  "contributors": [
    {
      "name": "Mathieu Welche (https://github.com/mwelche)",
      "email": "mathieuwelche@gmail.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/ubrien/web-video-editor/issues"
  },
  "homepage": "https://github.com/ubrien/web-video-editor#readme",
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "babel-plugin-istanbul": "^4.1.6",
    "chai": "4.1.2",
    "chai-immutable": "1.5.4",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "7.8.2",
